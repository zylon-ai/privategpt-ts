{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "extendedDiagnostics": true,
    "strict": true,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "declaration": true,
    "outDir": "dist",
    "rootDir": "src",
    "allowJs": true
  },
  "include": ["src"],
  "exclude": []
}
