{
  "name": "privategpt-sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/zylon-ai/privategpt-ts.git"
  },
  "version": "0.0.0",
  "description": "Privategpt SDK for web and nodejs",
  "scripts": {
    "format": "biome format --write ./src",
    "lint": "biome lint ./src",
    "build:web": "pnpm --filter privategpt-sdk-web build",
    "build:node": "pnpm --filter privategpt-sdk-node build",
    "build:utils": "pnpm --filter privategpt-sdk-utils build",
    "cp:package:web": "pnpm --filter privategpt-sdk-web cp:package",
    "cp:package:node": "pnpm --filter privategpt-sdk-node cp:package",
    "cp:package:utils": "pnpm --filter privategpt-sdk-utils cp:package"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "qs": "6.11.2",
    "react": "^18.3.1",
    "swr": "^2.2.5",
    "url-join": "4.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.2",
    "@types/node": "17.0.33",
    "@types/qs": "6.9.8",
    "@types/react": "^18.3.1",
    "@types/url-join": "4.0.1",
    "esbuild": "^0.21.1",
    "typescript": "4.6.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "license": "Apache-2.0"
}
